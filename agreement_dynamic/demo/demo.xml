<?xml version="1.0" encoding="utf-8" ?>
<odoo noupdate="1">
    <!-- Agreement -->
    <record id="demo_agreement_1" model="agreement">
        <field name="code">Unique code-01</field>
        <field name="name">Demo agreement</field>
        <field name="partner_id" ref="base.res_partner_12" />
        <field name="resource_ref" ref="hr.employee_mit" />
        <field name="start_date">2017-09-10</field>
        <field name="end_date">2118-09-10</field>
    </record>
    <!-- Sections -->
    <record id="demo_section_1" model="agreement.section">
        <field name="name">A paragraph</field>
        <field name="is_paragraph">True</field>
        <field name="content">Some Black content</field>
        <field name="python_code">object.name.startswith("D")</field>
        <field name="agreement_id" ref="demo_agreement_1" />
    </record>
    <record id="demo_section_2" model="agreement.section">
        <field name="name">Not a paragraph</field>
        <field name="is_paragraph">False</field>
        <field name="content">${object.name} ${page} ${object.name}</field>
        <field name="python_code">object.name.endswith("r")</field>
        <field name="agreement_id" ref="demo_agreement_1" />
    </record>
    <!-- Aliases -->
    <record id="demo_alias_1" model="agreement.dynamic.alias">
        <field name="expression_from">Black</field>
        <field name="expression_to">White</field>
        <field name="agreement_id" ref="demo_agreement_1" />
    </record>
    <record id="demo_alias_2" model="agreement.dynamic.alias">
        <field name="expression_from">${object.name}</field>
        <field name="expression_to">Name: ${object.name}</field>
        <field name="agreement_id" ref="demo_agreement_1" />
    </record>
</odoo>
